Host GitHub
	HostName github.com
	UseKeychain yes
	AddKeysToAgent yes
	User BinaryDennis
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/BinaryDennis-GitHub
	
Host Bitbucket
	HostName bitbucket.org
	UseKeychain yes
	AddKeysToAgent yes
	User mrcharmington
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/id_rsa_bookbeat

